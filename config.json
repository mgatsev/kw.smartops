{
  "settings": {
    "theme": "arc", // UI theme, possible values: fb (flat blue), wh (white), hc (high contrast) and other
    "autoSave": true, // auto-save open files
    
    "canOpen": true, // show Open button
    "canOpenDemo": false, // show Demo button
    "canOpenSettings": false, // show Settings button
    "canCreate": true, // show Create button
    "canImportXml": false, // show Import XML button

    "dropbox": false, // enable Dropbox
    "gdrive": false, // enable Google Drive
    "onedrive": false, // enable OneDrive
    "webdav": true, // enable WebDAV
  },
}
